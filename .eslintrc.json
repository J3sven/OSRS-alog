{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "max-classes-per-file": ["error", 3],
        "max-len": ["error", { "code": 200 }]
    }
}
