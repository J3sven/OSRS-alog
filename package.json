{
  "scripts": {
    "lint": "eslint js --ext .js",
    "lint:fix": "npm run lint -- --fix",
    "sass:compile": "node-sass --include-path sass css/main.scss css/main.css",
    "sass:watch": "node-sass --include-path sass css/main.scss css/main.css --watch --recursive",
    "sass:build": "node-sass --include-path sass css/main.scss css/main.css--output-style compressed",
    "start": "node app.js",
    "dev": "npm run sass:watch & nodemon app.js"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "firebase-admin": "^11.11.0",
    "fs": "^0.0.1-security",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "osrs-json-hiscores": "^2.16.0",
    "pluralize": "^8.0.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.1",
    "jsdoc": "^4.0.2",
    "node-sass": "^9.0.0",
    "nodemon": "^3.0.1",
    "prettier": "3.0.3"
  }
}
